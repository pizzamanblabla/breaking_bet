{% extends 'AppBundle:Common:base.html.twig' %}
{% block content %}
<div class="content">
    {% include 'AppBundle:Bets:filters.html.twig' %}
    <div class="bets-block">
        <table class="bets">
            <tr>
                <tr class="table-head">
                    <td>Sport</td>
                    <td>Chain</td>
                    <td>Event</td>
                    {% for type in coefficientTypes %}
                        <td>{{ type.name }}</td>
                    {% endfor %}
                </tr>
            </tr>
        </table>
    </div>
</div>
{% endblock %}